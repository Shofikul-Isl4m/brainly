{
  "version": 2,
  "rewrites": [
    {
      "source": "/user(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/share(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
