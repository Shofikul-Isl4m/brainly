{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/user/.*",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/share/.*",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": ".*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
